<form id="edit-form" class="form-horizontal form-ajax" role="form" data-toggle="validator" method="POST" action="">

    <div class="form-group">
        <label for="c-sid" class="control-label col-xs-12 col-sm-2">{:__('Sid')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-sid" required="" class="form-control" name="row[sid]" type="number" value="{$row.sid}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-muid" class="control-label col-xs-12 col-sm-2">{:__('Muid')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-muid" required="" class="form-control" name="row[muid]" type="number" value="{$row.muid}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-name" class="control-label col-xs-12 col-sm-2">{:__('Name')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-name" required="" class="form-control" name="row[name]" type="text" value="{$row.name}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-picpath" class="control-label col-xs-12 col-sm-2">{:__('Picpath')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-picpath" required="" class="form-control" name="row[picpath]" type="text" value="{$row.picpath}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-addtime" class="control-label col-xs-12 col-sm-2">{:__('Addtime')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-addtime" required="" class="form-control datetimepicker" data-date-format="YYYY-MM-DD HH:mm:ss" data-use-current="true" name="row[addtime]" type="text" value="{$row.addtime|datetime}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-uptime" class="control-label col-xs-12 col-sm-2">{:__('Uptime')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-uptime" required="" class="form-control datetimepicker" data-date-format="YYYY-MM-DD HH:mm:ss" data-use-current="true" name="row[uptime]" type="text" value="{$row.uptime|datetime}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-birthday" class="control-label col-xs-12 col-sm-2">{:__('Birthday')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-birthday" class="form-control" name="row[birthday]" type="text" value="{$row.birthday}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-jointime" class="control-label col-xs-12 col-sm-2">{:__('Jointime')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-jointime" class="form-control datetimepicker" data-date-format="YYYY-MM-DD HH:mm:ss" data-use-current="true" name="row[jointime]" type="text" value="{$row.jointime|datetime}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-gender" class="control-label col-xs-12 col-sm-2">{:__('Gender')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-gender" class="form-control" name="row[gender]" type="number" value="{$row.gender}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-position" class="control-label col-xs-12 col-sm-2">{:__('Position')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-position" class="form-control" name="row[position]" type="text" value="{$row.position}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-phone" class="control-label col-xs-12 col-sm-2">{:__('Phone')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-phone" class="form-control" name="row[phone]" type="text" value="{$row.phone}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-email" class="control-label col-xs-12 col-sm-2">{:__('Email')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-email" class="form-control" name="row[email]" type="text" value="{$row.email}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-address" class="control-label col-xs-12 col-sm-2">{:__('Address')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-address" class="form-control" name="row[address]" type="text" value="{$row.address}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-staff_type" class="control-label col-xs-12 col-sm-2">{:__('Staff_type')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-staff_type" class="form-control" name="row[staff_type]" type="number" value="{$row.staff_type}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-guesttime" class="control-label col-xs-12 col-sm-2">{:__('Guesttime')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-guesttime" class="form-control datetimepicker" data-date-format="YYYY-MM-DD HH:mm:ss" data-use-current="true" name="row[guesttime]" type="text" value="{$row.guesttime|datetime}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-expiration" class="control-label col-xs-12 col-sm-2">{:__('Expiration')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-expiration" class="form-control" name="row[expiration]" type="number" value="{$row.expiration}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-guest_status" class="control-label col-xs-12 col-sm-2">{:__('Guest_status')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-guest_status" class="form-control" name="row[guest_status]" type="number" value="{$row.guest_status}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-cor_depart" class="control-label col-xs-12 col-sm-2">{:__('Cor_depart')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-cor_depart" class="form-control" name="row[cor_depart]" type="text" value="{$row.cor_depart}">
        </div>
    </div>
    <div class="form-group">
        <label for="c-license" class="control-label col-xs-12 col-sm-2">{:__('License')}:</label>
        <div class="col-xs-12 col-sm-8">
            <input id="c-license" required="" class="form-control" name="row[license]" type="text" value="{$row.license}">
        </div>
    </div>
    <div class="form-group hide layer-footer">
        <label class="control-label col-xs-12 col-sm-2"></label>
        <div class="col-xs-12 col-sm-8">
            <button type="submit" class="btn btn-success btn-embossed">{:__('OK')}</button>
            <button type="reset" class="btn btn-default btn-embossed">{:__('Reset')}</button>
        </div>
    </div>
</form>
